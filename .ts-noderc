{
  "transpileOnly": true,
  "files": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "declaration": false
  }
}
